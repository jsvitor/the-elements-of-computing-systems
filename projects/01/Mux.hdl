/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Nand(a=b, b=sel, out=AnandSEL);
    Nand(a=sel, b=sel, out=notSEL);
    Nand(a=notSEL, b=a, out=SELnandB);
    Nand(a=AnandSEL, b=SELnandB, out=out);
}